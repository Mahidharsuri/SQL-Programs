CREATE TABLE    FACEBOOK (USERNAME VARCHAR(50),FOLLOWERS INT ,FOLOWING INT,N_POSTS INT,LIKES VARCHAR2(30) ,DOB DATE ,COUNTRY CHAR(30));

SELECT * FROM FACEBOOK;
 


