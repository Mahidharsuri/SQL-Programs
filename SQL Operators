SELECT * FROM FACEBOOK WHERE DOB BETWEEN '17-07-2002' AND '10-12-2004';
SELECT* FROM FACEBOOK WHERE COUNTRY BETWEEN 'IND' AND 'US';

SELECT * FROM FACEBOOK WHERE USERNAME BETWEEN 'MAX' AND 'NEHI';

SELECT * FROM  FACEBOOK WHERE   N_POSTS >=10 AND N_POSTS<=20;

SELECT * FROM FACEBOOK WHERE N_POSTS BETWEEN 10 AND 15;
SELECT  N_POSTS,LIKES,DOB FROM FACEBOOK WHERE LIKES BETWEEN '2K' AND '9K';

SELECT * FROM FACEBOOK WHERE  LIKES='2K'  ;
SELECT * FROM  FACEBOOK WHERE N_POSTS IN (11,17);
SELECT * FROM  FACEBOOK WHERE N_POSTS NOT IN (11,17);

SELECT * FROM FACEBOOK WHERE USERNAME LIKE 'M%X';
SELECT * FROM FACEBOOK WHERE USERNAME NOT LIKE '%X';
SELECT * FROM FACEBOOK WHERE  LIKES!='2K'  ;
SELECT * FROM FACEBOOK WHERE COUNTRY LIKE '%Z';

