CREATE TABLE INSTAGRAM (USERNAME VARCHAR(50),FOLLWERS INT ,FOLOWING INT,N_POSTS INT, COUNTRY CHAR(30));

 

INSERT INTO INSTAGRAM VALUES('CHITTI',570,34,50,'US');
INSERT INTO INSTAGRAM VALUES('KUSUMA',5000,30,10,'IND');
INSERT INTO INSTAGRAM VALUES('ISHU',1500,50,5,'IND');
INSERT INTO INSTAGRAM VALUES('PRAVS',2500,0,0,'SW');
INSERT INTO INSTAGRAM VALUES('YOGI',5200,90,40,'CAN');
INSERT INTO INSTAGRAM VALUES('RAVI',1500,301,56,'NY');
INSERT INTO INSTAGRAM VALUES('MAX',500,300,50,'IND');

DELETE  FROM  INSTAGRAM  WHERE COUNTRY='US';

UPDATE INSTAGRAM SET N_POSTS=22 WHERE USERNAME='MAX';

SELECT * FROM INSTAGRAM WHERE N_POSTS>30 AND N_POSTS<=300;

SELECT * FROM INSTAGRAM WHERE COUNTRY BETWEEN 'IND' AND 'NY';

 SELECT USERNAME FROM INSTAGRAM WHERE N_POSTS BETWEEN 22 AND 56;
 SELECT * FROM INSTAGRAM;
 
 SELECT 10+20;
 
 SELECT * FROM INSTAGRAM WHERE NOT N_POSTS=32;
 
 SELECT * FROM INSTAGRAM WHERE N_POSTS NOT BETWEEN 10 AND 22;
 
 SELECT FOLLWERS,FOLOWING FROM INSTAGRAM WHERE FOLOWING=50 OR FOLOWING =90 OR FOLLWERS = 500;
 
  SELECT FOLLWERS,FOLOWING,COUNTRY FROM INSTAGRAM WHERE FOLOWING  IN (50,90,300);

  SELECT FOLLWERS,FOLOWING,COUNTRY FROM INSTAGRAM WHERE FOLOWING NOT IN (50,90,300); 
  
SELECT * FROM INSTAGRAM WHERE USERNAME LIKE 'Y%I';

